    <!-- Suppress test-scope vulnerabilities that don't affect production -->
    <suppress>
        <notes><![CDATA[
            Test-scope dependencies with vulnerabilities that don't affect production builds
        ]]></notes>
        <gav regex="true">^.*:.*:.*$</gav>
        <cpe>cpe:/a:junit:junit</cpe>
    </suppress>
    
</suppressions>
