{
  "name": "xstr-api-client",
  "version": "1.0.0",
  "description": "TypeScript client library for Xstr.me API - Generated from OpenAPI specification",
  "author": "Xstr.me Development Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/xstr.me/api-spec.git"
  },
  "homepage": "https://github.com/xstr.me/api-spec#readme",
  "bugs": {
    "url": "https://github.com/xstr.me/api-spec/issues"
  },
  "keywords": [
    "xstr-api",
    "typescript",
    "axios",
    "openapi-client",
    "openapi-generator",
    "rest-api",
    "http-client",
    "vue",
    "react",
    "angular",
    "frontend"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "CHANGELOG.md"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "clean": "rimraf dist coverage .nyc_output",
    "prebuild": "npm run clean",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:watch": "tsc -p tsconfig.json --watch",
    "lint": "eslint \"**/*.{ts,js}\" --fix",
    "lint:check": "eslint \"**/*.{ts,js}\"",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "type-check": "tsc --noEmit",
    "validate": "npm run lint:check && npm run format:check && npm run type-check && npm run test:ci",
    "prepare": "npm run build",
    "prepublishOnly": "npm run validate && npm run build",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "postversion": "git push && git push --tags",
    "release": "np --no-yarn",
    "docs": "typedoc --out docs src/index.ts"
  },
  "dependencies": {
    "axios": "^1.6.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "conventional-changelog-cli": "^4.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "np": "^10.0.2",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "axios": ">=0.27.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
