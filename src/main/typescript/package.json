{
  "name": "xstr-api-client",
  "version": "1.0.0",
  "description": "TypeScript client for xstr.me API - Generated from OpenAPI specification",
  "author": "xstr.me",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xstr-me/api-client.git"
  },
  "homepage": "https://xstr.me",
  "bugs": {
    "url": "https://github.com/xstr-me/api-client/issues"
  },
  "keywords": [
    "xstr",
    "api",
    "client",
    "typescript",
    "openapi",
    "rest"
  ],
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md",
    "package.json"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "npm run clean && npm run build:cjs && npm run build:esm",
    "build:cjs": "npx tsc -p tsconfig.json",
    "build:esm": "npx tsc -p tsconfig.esm.json",
    "clean": "npx rimraf dist",
    "test": "cd ../../test/typescript && npx jest basic.test.ts",
    "test:watch": "cd ../../test/typescript && npm run test:watch",
    "test:coverage": "cd ../../test/typescript && npm run test:coverage",
    "test:all": "cd ../../test/typescript && npm test",
    "lint": "npx eslint src/**/*.ts",
    "lint:fix": "npx eslint src/**/*.ts --fix",
    "format": "npx prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "npx prettier --check \"src/**/*.{ts,js,json}\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "preversion": "npm run lint && npm run test",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "cross-fetch": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
