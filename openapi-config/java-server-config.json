{
    "library": "spring-boot",
    "groupId": "me.xstr",
    "artifactId": "xstr-api-server",
    "artifactVersion": "0.0.1-alpha",
    "basePackage": "me.xstr.api.server",
    "configPackage": "me.xstr.api.server.config",
    "apiPackage": "me.xstr.api.server.api",
    "modelPackage": "me.xstr.api.server.model",
    "dateLibrary": "java8",
    "java8": true,
    "hideGenerationTimestamp": true,
    "generateApiTests": true,
    "generateModelTests": true,
    "useBeanValidation": true,
    "performBeanValidation": true,
    "useJakartaEe": true,
    "serializableModel": true,
    "withXml": false,
    "interfaceOnly": true,
    "delegatePattern": true,
    "useTags": true,
    "singleContentTypes": false,
    "skipDefaultInterface": false,
    "reactive": false,
    "openApiNullable": false,
    "additionalModelTypeAnnotations": "@com.fasterxml.jackson.annotation.JsonInclude(com.fasterxml.jackson.annotation.JsonInclude.Include.NON_NULL)"
}